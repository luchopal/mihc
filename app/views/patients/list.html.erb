<h1>Pacientes</h1>

<div class="row">
    <table class="table">
      <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>DNI</th>
        <th>Obra Social/Num</th>
        <th>Accion</th>
      </tr>
      <% @patients.each do |data| %>
          <tr>
            <td><%= data.name %></td>
            <td><%= data.surname %></td>
            <td><%= data.document_number %></td>
            <td><%= data.associate_number %></td>
            <td><b> <%= link_to ('<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>').html_safe, {:action => "edit", :id => data.id}%></b>
                <b> <%= link_to ('<span class="glyphicon glyphicon-plus" aria-hidden="true"></span>').html_safe, medical_consultations_list_path(data) , method: :get %></b>
            </td>
          </tr>
      <% end %>
    </table>
</div>
<p><%= link_to "Agregar nuevo paciente", {:action => 'new' }, class: "btn btn-primary"%></p>