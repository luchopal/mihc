                                    </tr>
                                    <tr>
                                      <td align="center"><%= inguinocrural_examination.check_box :right_coercible_crural_hernia,  :data => { :toggle => 'toggle', :on => 'Si', :off => 'No', :size => 'mini' } %></td>
